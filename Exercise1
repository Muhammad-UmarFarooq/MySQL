-- Create table

CREATE TABLE personal
  (
  id INT,
  name VARCHAR(50),
  gender VARCHAR(1),
  phone VARCHAR(12),
  dob DATE
  );

-- Insert record-1

INSERT INTO personal(id,name,gender,phone,dob)
VALUES(1,"RaviKumar","M","9494989796",'1989-05-04');

-- Retrieve data

select * from personal;

